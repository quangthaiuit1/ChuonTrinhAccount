package trong.lixco.com.entity;

import javax.persistence.Entity;

/*
 * Danh muc don vi
 */
@Entity
public class Department extends AbstractEntity {
	private String code;
	private String name;
	private boolean cnbd=false;
	

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public boolean isCnbd() {
		return cnbd;
	}

	public void setCnbd(boolean cnbd) {
		this.cnbd = cnbd;
	}
	


}
